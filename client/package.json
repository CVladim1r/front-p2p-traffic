    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-ga4": "^2.1.0",
    "react-lottie": "^1.2.10",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "serve": "^14.2.4",
    "tailwind": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/crypto-js": "^4.2.2",
